<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context="com.itremedy.improvidermtfree.PortsSelectActivity">

    <TextView
        android:id="@+id/textViewInPremium"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/abhaya_libre_semibold"
        android:text="@string/in_premium_version"
        android:textColor="@color/text_color"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5ports" />

    <LinearLayout
        android:id="@+id/linearLayoutHeader"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/dark_background_card_view"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textViewHeader"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:fontFamily="@font/abhaya_libre_semibold"
            android:gravity="center"
            android:text="@string/how_many_ethernet_ports"
            android:textColor="@color/text_color"
            android:textSize="20sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5ports"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/dark_background_card_view"
        android:clickable="true"
        android:focusable="auto"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayoutHeader">

        <TextView
            android:id="@+id/textView5ports"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="10dp"
            android:layout_weight="1"
            android:gravity="center_vertical|end"
            android:text="5"
            android:textColor="@color/text_color"
            android:textSize="34sp" />

        <ImageView
            android:id="@+id/imageView5ports"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginEnd="10dp"
            app:srcCompat="@drawable/_5_port" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout8ports"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/dark_background_card_view"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewInPremium">

        <TextView
            android:id="@+id/textView8ports"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="10dp"
            android:layout_weight="1"
            android:gravity="center_vertical|end"
            android:text="8"
            android:textColor="@color/text_color"
            android:textSize="34sp" />

        <ImageView
            android:id="@+id/imageView8ports"
            android:layout_width="220dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginEnd="10dp"
            app:srcCompat="@drawable/_8_port" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout10ports"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/dark_background_card_view"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout8ports">

        <TextView
            android:id="@+id/textView10ports"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical|end"
            android:text="10"
            android:textColor="@color/text_color"
            android:textSize="34sp" />

        <ImageView
            android:id="@+id/imageView10ports"
            android:layout_width="250dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/_10_port" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout12ports"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/dark_background_card_view"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10ports">

        <TextView
            android:id="@+id/textView12ports"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical|end"
            android:text="12"
            android:textColor="@color/text_color"
            android:textSize="34sp" />

        <ImageView
            android:id="@+id/imageView12ports"
            android:layout_width="260dp"
            android:layout_height="55dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/_12_port" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout16ports"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/dark_background_card_view"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout12ports">

        <TextView
            android:id="@+id/textView16ports"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical|end"
            android:text="16"
            android:textColor="@color/text_color"
            android:textSize="34sp" />

        <ImageView
            android:id="@+id/imageView16ports"
            android:layout_width="260dp"
            android:layout_height="55dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/_16_port" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout24ports"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        android:background="@drawable/dark_background_card_view"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout16ports">

        <TextView
            android:id="@+id/textView24ports"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical|end"
            android:text="24"
            android:textColor="@color/text_color"
            android:textSize="34sp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="260dp"
            android:layout_height="55dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/_24_port" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>